{
  "name": "network-manager",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon main.js",
    "test": "jest --coverage",
    "lint": "eslint . --fix",
    "build": "tsc",
    "install-dependencies": "npm install",
    "update": "sh scripts/update.sh",
    "uninstall": "sh scripts/uninstall.sh"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "electron": "^28.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "systeminformation": "^5.21.7",
    "winston": "^3.8.2",
    "pm2": "^5.3.0",
    "socket.io": "^4.7.1",
    "uuid": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^20.9.1",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "supertest": "^6.3.3",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "appId": "com.kalfanet.networkmanager",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "Agent",
    "PC-MAC-Manager",
    "Remote-Control",
    "Network-Monitoring"
  ],
  "author": "KALFANET",
  "license": "MIT"
}